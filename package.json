{
  "name": "react-email-workflow",
  "version": "1.0.0",
  "description": "Email newsletter design workflow. Built with React, Redux, Express, Webpack, JSXStyle, React Drag and Drop, Premailer.",
  "main": "server.js",
  "scripts": {
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --report lcov node_modules/mocha/bin/_mocha -- --reporter dot",
    "dist": "webpack --config webpack.config.js",
    "build": "npm run clean && npm run dist",
    "clean": "rm -rf dist",
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development babel-node server.js",
    "start:prod": "NODE_ENV=production babel-node server.js",
    "test": "mocha --compilers js:babel-core/register --recursive && npm run coverage",
    "test:watch": "npm test -- --watch",
    "lint": "eslint src",
    "extract": "babel-node services/extract",
    "premail": "babel-node services/premail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaredpalmer/react-email-workflow.git"
  },
  "keywords": [
    "react",
    "email",
    "redux",
    "react-DnD",
    "babel",
    "webpack",
    "jsxstyle"
  ],
  "author": "Jared Palmer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaredpalmer/react-email-workflow/issues"
  },
  "homepage": "https://github.com/jaredpalmer/react-email-workflow#readme",
  "dependencies": {
    "axios": "0.12.0",
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "body-parser": "1.15.2",
    "classnames": "2.2.5",
    "clipboard": "1.5.12",
    "codemirror": "5.16.0",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "expect": "1.20.1",
    "express": "4.14.0",
    "helmet": "2.1.1",
    "jackrabbit": "4.3.0",
    "jsxstyle": "petehunt/jsxstyle",
    "lodash": "4.13.1",
    "logfmt": "1.2.0",
    "moment": "2.13.0",
    "node-uuid": "1.4.7",
    "premailer-api": "1.0.4",
    "react": "15.1.0",
    "react-dnd": "2.1.4",
    "react-dnd-html5-backend": "2.1.2",
    "react-dom": "15.1.0",
    "react-redux": "4.4.5",
    "react-textarea-autosize": "4.0.2",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "throng": "4.0.0",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "babel-eslint": "6.0.5",
    "codeclimate-test-reporter": "^0.3.3",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-react": "5.2.2",
    "isparta": "4.0.0",
    "mocha": "2.5.3",
    "react-hot-loader": "^1.3.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "engines": {
    "node": "6.2.1"
  }
}
