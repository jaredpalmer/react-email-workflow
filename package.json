{
  "name": "react-email-workflow",
  "version": "1.0.0",
  "description": "Email workflow for ShellyPalmer. Built with React, Redux, Express, Webpack, React-Transform, CSS Modules.",
  "main": "server.js",
  "scripts": {
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --report text --report html --report lcov node_modules/mocha/bin/_mocha -- --reporter dot",
    "dist": "webpack --config webpack.config.js",
    "build": "npm run clean && npm run dist",
    "clean": "rm -rf public/scripts",
    "start": "npm run start:dev",
    "start:dev": "node serverDev.js",
    "start:prod": "NODE_ENV=production node server.js",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test:watch": "npm test -- --watch",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaredpalmer/react-email-workflow.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaredpalmer/react-email-workflow/issues"
  },
  "homepage": "https://github.com/jaredpalmer/react-email-workflow#readme",
  "dependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-syntax-decorators": "^6.3.13",
    "babel-plugin-transform-decorators": "^6.4.0",
    "babel-preset-airbnb": "^1.0.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "codeclimate-test-reporter": "^0.3.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-react": "^3.16.1",
    "expect": "^1.13.4",
    "express": "^4.13.4",
    "istanbul": "^0.4.2",
    "jsxstyle": "0.0.18",
    "lodash.range": "^3.1.0",
    "mocha": "^2.4.2",
    "node-uuid": "^1.4.7",
    "react": "^0.14.6",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.2",
    "react-dom": "^0.14.6",
    "react-motion": "^0.4.1",
    "react-redux": "^4.0.6",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redux": "^3.0.6",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "devDependencies": {
    "isparta": "^4.0.0"
  }
}
