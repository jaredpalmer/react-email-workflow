{
  "name": "react-email-workflow",
  "version": "1.0.0",
  "description": "Email newsletter design workflow. Built with React, Redux, Express, Webpack, JSXStyle, React Drag and Drop, Premailer.",
  "main": "server.js",
  "scripts": {
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --report lcov node_modules/mocha/bin/_mocha -- --reporter dot",
    "dist": "webpack --config webpack.config.js",
    "build": "npm run clean && npm run dist",
    "clean": "rm -rf dist",
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development babel-node server.js",
    "start:prod": "NODE_ENV=production babel-node server.js",
    "test": "mocha --compilers js:babel-core/register --recursive && npm run coverage",
    "test:watch": "npm test -- --watch",
    "lint": "eslint src",
    "extract": "babel-node services/extract",
    "premail": "babel-node services/premail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaredpalmer/react-email-workflow.git"
  },
  "keywords": [
    "react",
    "email",
    "redux",
    "react-DnD",
    "babel",
    "webpack",
    "jsxstyle"
  ],
  "author": "Jared Palmer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaredpalmer/react-email-workflow/issues"
  },
  "homepage": "https://github.com/jaredpalmer/react-email-workflow#readme",
  "dependencies": {
    "axios": "0.11.0",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-react-constant-elements": "6.5.0",
    "babel-plugin-transform-react-inline-elements": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "body-parser": "^1.14.2",
    "classnames": "2.2.3",
    "clipboard": "^1.5.5",
    "codeclimate-test-reporter": "^0.3.0",
    "codemirror": "5.11.0",
    "compression": "1.6.1",
    "cookie-parser": "^1.4.1",
    "expect": "^1.13.4",
    "express": "^4.13.4",
    "fbjs": "^0.7.0",
    "helmet": "^1.1.0",
    "isparta": "^4.0.0",
    "jackrabbit": "4.3.0",
    "jsxstyle": "0.0.18",
    "logfmt": "^1.2.0",
    "mocha": "^2.4.2",
    "moment": "^2.11.1",
    "node-uuid": "1.4.7",
    "premailer-api": "^1.0.3",
    "react": "^0.14.6",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.2",
    "react-dom": "^0.14.6",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.6",
    "react-textarea-autosize": "3.3.0",
    "redux": "^3.0.6",
    "redux-thunk": "^1.0.3",
    "superagent": "^1.7.2",
    "throng": "4.0.0",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.8",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.1",
    "eslint-plugin-react": "^3.16.1"
  }
}
